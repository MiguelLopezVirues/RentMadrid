[('transformer', ColumnTransformer(remainder='passthrough',
                  transformers=[('one_hot',
                                 OneHotEncoder(drop='first',
                                               sparse_output=False),
                                 ['exterior', 'bathrooms',
                                  'municipality_grouped']),
                                ('target', TargetEncoder(),
                                 ['rooms', 'floor_grouped'])])), ('regressor', GradientBoostingRegressor(learning_rate=0.04, max_depth=8, min_samples_leaf=5,
                          min_samples_split=50, n_estimators=70, subsample=0.9))]